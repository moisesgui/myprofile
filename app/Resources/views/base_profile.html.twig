{% trans_default_domain 'MyProfile' %}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="author" content="My Profile">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Welcome!{% endblock %} | My profile</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('profile') }}
    {% endblock %}

    <link rel="shortcut icon" href="{{ asset('assets/images/ico/favicon.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('assets/images/ico/apple-touch-icon-144-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset('assets/images/ico/apple-touch-icon-114-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('assets/images/ico/apple-touch-icon-72-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" href="{{ asset('assets/images/ico/apple-touch-icon-57-precomposed.png') }}">
</head>
<body>
{% include 'default/partials/flashmessages.html.twig' %}



{% include 'navegation_profile.html.twig' %}
<div class="container">
    {% block container_profile %}
    {% endblock container_profile %}
</div>

{% block javascripts %}
    {{ encore_entry_script_tags('profile') }}
{% endblock %}
</body>
</html>